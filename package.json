{
  "name": "hnfzf",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "dev:server": "npx ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:server:simple": "npx ts-node src/server.ts",
    "dev:server:watch": "npx ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
    "dev:frontend": "npx vite dev src/app",
    "build": "tsc ",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:ingest": "docker-compose run --rm ingest",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "db:setup": "./scripts/setup-db.sh setup",
    "db:start": "./scripts/setup-db.sh setup",
    "db:stop": "docker stop hnfzf-db-dev",
    "db:restart": "./scripts/setup-db.sh restart",
    "db:clean": "./scripts/setup-db.sh clean",
    "db:logs": "./scripts/setup-db.sh logs",
    "db:connect": "./scripts/setup-db.sh connect",
    "db:status": "./scripts/setup-db.sh status",
    "db:test": "node scripts/db-utils.js test",
    "db:seed": "node scripts/db-utils.js seed",
    "db:init": "node scripts/db-utils.js init",
    "db:check": "node scripts/db-utils.js status",
    "validate": "node scripts/validate-setup.js",
    "dev:setup": "npm run db:setup && npm run dev:server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.18",
    "@types/pg": "^8.15.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}